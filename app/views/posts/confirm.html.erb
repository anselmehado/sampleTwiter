<h3>Les informations suivantes doivent être envoyées.</h3>
<%= form_with(model: @post, url: posts_path, local: true) do |form| %>
  <%= form.hidden_field :Autheur %>
  <%= form.hidden_field :content %>
  <p>タイトル:<%= @post.Autheur %></p>
  <p>本文:<%= @post.content %></p>
  <%= form.submit  "Registre" %>
    <%= form.submit "戻る" %>
<% end %>
<%= link_to 'Back', :back %>
