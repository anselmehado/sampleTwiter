<%= form_with(model: @post, local: true) do |form| %>

  <div class="post_Autheur">
    <%= form.label :Autheur %>
    <%= form.text_field :Autheur %>
  </div>
  <div class="post_content">
    <%= form.label :content %>
    <%= form.text_field :content %>
  </div>
  <%= form.submit %>
    <% end %>
